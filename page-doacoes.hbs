{{!< default}}
{{!-- The tag above means: insert everything in this file
into the {body} of the default.hbs template --}}

{{!-- The big featured header, it uses blog cover image as a BG if available --}}
<header class="site-header">
  <div class="outer site-nav-main">
    <div class="inner">
      {{> "site-nav"}}
    </div>
  </div>
</header>

{{!-- Everything inside the #post tags pulls data from the post --}}
{{#post}}

  <main id="site-main" class="site-main outer">
    <div class="inner">

      <article class="post-full {{post_class}} {{#unless feature_image}}no-image{{/unless}}">

        {{#if feature_image}}
          <figure class="post-full-image">
            {{!-- This is a responsive image, it loads different sizes depending on device
                https://medium.freecodecamp.org/a-guide-to-responsive-images-with-ready-to-use-templates-c400bd65c433 --}}
            <img srcset="{{img_url feature_image size="s"}} 300w,
                            {{img_url feature_image size="m"}} 600w,
                            {{img_url feature_image size="l"}} 1000w,
                            {{img_url feature_image size="xl"}} 2000w" sizes="(max-width: 800px) 400px,
                            (max-width: 1170px) 1170px,
                            2000px" src="{{img_url feature_image size="xl"}}" alt="{{title}}" />
          </figure>
        {{/if}}

        <header class="post-full-header">
          <h1 class="post-full-title">{{title}}</h1>
        </header>

        <section class="post-full-content">
          <div class="post-content">
            {{content}}
          </div>
        </section>

        <section class="recurring no-spacing">
          <div class="checkout-form">
            <h1 class="checkout-title">Apoio recorrente</h1>
            <p>Para quem quiser me apoiar constantemente</p>
            <div class="checkout-box">

              <div class="checkout-plan">
                <header class="checkout-plan-header">
                  <h3>PicPay</h3>
                  <span>disponível em: https://picpay.me/khaosdoctor</span>
                </header>
                <div class="checkout-plan-content">
                  <ul>
                    <li>Pagamento seguro</li>
                    <li>Suporta muitos meios</li>
                    <li>Você pode cancelar a qualquer momento</li>
                  </ul>
                  <a class="button primary fit" rel="noopener noreferrer nofollow external" target="_blank"
                     onclick="sendLeadEvent('PicPay Page Doações')" href="https://picpay.me/khaosdoctor" data-members-plan="Monthly">Ver
                    planos</a>
                </div>
              </div>

              <div class="checkout-plan">
                <header class="checkout-plan-header">
                  <h3>Apoia.se</h3>
                  <span>disponível em: https://apoia.se/lsantos</span>
                </header>
                <div class="checkout-plan-content">
                  <ul>
                    <li>Pagamento seguro</li>
                    <li>Suporta muitos meios</li>
                    <li>Você pode cancelar a qualquer momento</li>
                  </ul>
                  <a class="button primary fit" rel="noopener noreferrer nofollow external" target="_blank"
                     onclick="sendLeadEvent('Apoia.se Page Doações')" href="https://apoia.se/lsantos" data-members-plan="Monthly">Ver
                    planos</a>
                </div>
              </div>
            </div>
          </div>
        </section>

        <section class="individual no-spacing">
          <div class="checkout-form">
            <h1 class="checkout-title">Apoio único</h1>
            <p>Para quem quiser me apoiar em apenas uma doação</p>
            <div class="checkout-box">

              <div class="checkout-plan bt-green">
                <header class="checkout-plan-header">
                  <h3>PayPal</h3>
                  <span>Doe o quanto quiser!</span>
                </header>
                <div class="checkout-plan-content no-spacing">
                  <a class="button primary fit" rel="noopener noreferrer nofollow external" target="_blank"
                     onclick="sendLeadEvent('PayPal Page Doações')" href="https://rb.gy/dqhi1t">Doar</a>
                </div>
              </div>

              <div class="checkout-plan bt-green">
                <header class="checkout-plan-header">
                  <h3>Tippin.me</h3>
                  <span>Doe o quanto quiser!</span>
                </header>
                <div class="checkout-plan-content no-spacing">
                  <a class="button primary fit" rel="noopener noreferrer nofollow external" target="_blank"
                     onclick="sendLeadEvent('Tippin Page Doações')" href="https://tippin.me/@_StaticVoid">Doar</a>
                </div>
              </div>

              <div class="checkout-plan bt-green">
                <header class="checkout-plan-header">
                  <h3>BuyMeACoffee</h3>
                  <span>Doe o quanto quiser!</span>
                </header>
                <div class="checkout-plan-content no-spacing">
                  <a class="button primary fit" onclick="sendLeadEvent('BMC Page Doações')" target="_blank"
                     href="https://www.buymeacoffee.com/lsantos">Doar</a>
                </div>
              </div>
            </div>
          </div>
        </section>

        <section class="individual no-spacing">
          <div class="checkout-form">
            <h1 class="checkout-title">Apoio único com criptomoedas</h1>
            <p>Para quem quiser me apoiar em apenas uma doação através de criptocoins</p>
            <div class="checkout-box">

              <div class="checkout-plan bt-red">
                <header class="checkout-plan-header">
                  <h3>Bitcoin</h3>
                  <span>Doe o quanto quiser!</span>
                </header>
                <div class="checkout-plan-content no-spacing">
                  <pre>3NWNzrrbw2NaBrDZQzSnL739vwvbSfJmMv</pre>
                  <img src="data:image/png;base64,"
                       alt="Carteira Bitcoin">
                </div>
              </div>

              <div class="checkout-plan bt-red">
                <header class="checkout-plan-header">
                  <h3>Ethereum</h3>
                  <span>Doe o quanto quiser!</span>
                </header>
                <div class="checkout-plan-content no-spacing">
                  <pre>0x64f235dF95CD5d53659F5B315eC4e3f1B11a218c</pre>
                  <img src="data:image/png;base64,"
                       alt="Carteira Ethereum">
                </div>
              </div>

              <div class="checkout-plan bt-red">
                <header class="checkout-plan-header">
                  <h3>Litecoin</h3>
                  <span>Doe o quanto quiser!</span>
                </header>
                <div class="checkout-plan-content no-spacing">
                  <pre>Lg91EHEg7VzLUzD3bExuLMTSXVcwnozmik</pre>
                  <img src="data:image/png;base64,"
                       alt="Carteira Litecoin">
                </div>
              </div>

            </div>
          </div>
        </section>
      </article>

    </div>
  </main>


  {{!-- The #contentFor helper here will send everything inside it up to the matching #block helper found in default.hbs --}}
  {{#contentFor "scripts"}}
    <script>
      $(function () {
        var $postContent = $(".post-full-content");
        $postContent.fitVids();
      });
      sendViewPostEvent('{{id}}', 'page', '{{title}}')
    </script>
  {{/contentFor}}

{{/post}}
